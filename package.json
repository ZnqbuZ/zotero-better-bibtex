    "emittery": "^1.0.1",
    "fast-safe-stringify": "^2.1.1",
    "fold-to-ascii": "^5.0.1",
    "html-to-md": "^0.8.3",
    "i": "^0.3.7",
    "jieba-zh-cn": "^1.0.12",
